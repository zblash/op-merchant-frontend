{
  "name": "op-merchant-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "yarn generate-type && yarn run clean-dist && cross-env NODE_ENV=production webpack -p --config=webpack/production.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint ./src/**/*.ts* ./tests/**/*.test.ts* --cache",
    "prettier": "prettier  --ignore-path ./.prettierignore  **/*.ts*",
    "generate-type": "node scripts/type-generator.js",
    "generate-type-watch": "nodemon -e json --watch statics --exec \"yarn generate-type\"",
    "develop": "concurrently \"yarn generate-type-watch\" \"yarn dev\"",
    "fix": "yarn prettier --write && yarn lint --fix",
    "tsc": "tsc",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/development.js",
    "start": "node server.js",
    "test": "jest --coverage  --config=jest.json",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 9009 -c stories/config -s public",
    "build-storybook": "cross-env NODE_ENV=development build-storybook -s public",
    "check-rule": "yarn tsc && yarn lint && yarn prettier && yarn test"
  },
  "devDependencies": {
